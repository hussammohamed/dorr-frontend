    <div class="m-header layout-row">
        <div class="m-header--item m-header--item__logo flex-10">
            <a href="/" >


                <img src="/images/dorr-logo.svg" alt="Dorr">
            </a>
        </div>
        <div class="flex bread-crumb">
        {{component 'bread-crumbs-component' }}
        </div>
        <div class="m-header--item m-header--item__user mdl-cell--hide-phone flex">
            <div class="user-menu mdl-cell--hide-phone" id="userMenu">
                <div class="user-menu--overlay"></div>
                <div class="user-menu--header">
                    <i class="material-icons collapse-icon"></i>
                    <div class="user-menu--avatar mdl-avatar">
                        <img src={{if user.avatar user.avatar "http://dorr.bareeqstudio.com/images/face.png"}}>
                    </div>
                    <div class="user-info">
                        <div class="user-name"> {{user.name}}</div>
                        <div class="user-mony">
                            <i class="material-icons">fiber_smart_record</i>
                            <span>10,000</span>
                        </div>
                    </div>
                </div>
                <ul class="user-action-list">
                    <li>
                        <a href="{{mainUrl}}/myAccount">حسابي</a>
                    </li>
                    <li>
                        <a href="{{mainUrl}}/myAccount/Properties">إعلانتي</a>
                    </li>
                    {{!-- <li>
                        <a href="#">إدارة العقارات</a>
                    </li> --}}
                    <li>
                        <a  onClick={{action "logout"}}>خروج</a>
                        
                        
                    </li>

                </ul>
            </div>
        </div>
        
        
    </div>
